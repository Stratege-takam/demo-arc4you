	<ItemGroup>
	  <Compile Update="Pages\Resources\About.Designer.cs">
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	    <DependentUpon>About.resx</DependentUpon>
	  </Compile>
	  <Compile Update="Pages\Resources\Global.Designer.cs">
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	    <DependentUpon>Global.resx</DependentUpon>
	  </Compile>
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Update="Pages\Resources\About.resx">
	    <Generator>PublicResXFileCodeGenerator</Generator>
	    <LastGenOutput>About.Designer.cs</LastGenOutput>
	  </EmbeddedResource>
	  <EmbeddedResource Update="Pages\Resources\Global.resx">
	    <Generator>PublicResXFileCodeGenerator</Generator>
	    <LastGenOutput>Global.Designer.cs</LastGenOutput>
	  </EmbeddedResource>
	</ItemGroup>

  <PropertyGroup>
    <NoDefaultLaunchSettingsFile>true</NoDefaultLaunchSettingsFile>
