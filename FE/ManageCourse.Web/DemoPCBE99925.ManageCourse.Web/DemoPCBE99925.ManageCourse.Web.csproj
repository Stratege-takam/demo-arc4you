	<ItemGroup>
	  <ProjectReference Include="..\..\..\BE\ManageCourseService\Sdks\DemoPCBE99925.ManageCourseService.Facade.Sdk\DemoPCBE99925.ManageCourseService.Facade.Sdk.csproj" />
