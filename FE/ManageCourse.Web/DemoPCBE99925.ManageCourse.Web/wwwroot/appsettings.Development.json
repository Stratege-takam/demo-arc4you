{
  "Application.Configuration": {
    "ApplicationName": "ManageCourse.Web",
    "Environment": {
      "Name": "Development",
      "LoggingName": "Blazor.ManageCourse.Web",
      "TimeZone": "Romance Standard Time",
      "LoggingLevel": "Trace"
    }
  },
  "AppSettings": {
    "GatewayServiceUrl": "https://localhost:7206/"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.BrowserConsole",
      "Serilog.Expressions"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Grpc": "Warning"
      }
    },
    "WriteTo:Console": {
      "Name": "Logger",
      "Args": {
        "configureLogger": {
          "MinimumLevel": "Information",
          "Filter": [
            {
              "Name": "ByExcluding",
              "Args": {
                "expression": "Category = 4"
              }
            }
          ],
          "WriteTo": [
            {
              "Name": "BrowserConsole",
              "Args": {
                "outputTemplate": "[{Timestamp:HH:mm:ss} {SourceContext} [{Level}] {Message}{NewLine}{Exception}",
                "theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Grayscale, Serilog.Sinks.Console"
              }
            }
          ]
        }
      }
    },
    "Properties": {
      "Application": "Blazor.ManageCourse.Web"
    }
  },
  "Blazor.OAuth2.ManageCourse.Web.Settings": {
    "ProviderId": "blazor",
    "AuthenticationType": "OAuth2Bearer",
    "Authority": "https://adfsdev.belgrid.net/adfs",
    "ClientId": "274f12b5-f635-4980-89c9-f5f453bee451",
    "ValidateAuthority": true,
    "Scopes": "https://demopcbe99925.development.host/user_impersonation"
  }
}
